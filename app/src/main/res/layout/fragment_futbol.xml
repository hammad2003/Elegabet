<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="futbolRowModel" type="com.hammadjamilsapplication.app.modules.futbol.data.model.FutbolRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumn"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnsettings1"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_8pxv"
            android:layout_marginEnd="@dimen/_39pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <LinearLayout android:id="@+id/linearRowpremierleague"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnpremierleague"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.87"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPremierLeague"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{futbolRowModel.txtPremierLeague}"
                        style="@style/txtSFUIDisplayregular10" tools:text="@string/lbl_premier_league"/>
                    <LinearLayout android:id="@+id/linearRowzero"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowbara"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.86"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageBara"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_bara"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_bara"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtBarcelona"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.82"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{futbolRowModel.txtBarcelona}"
                                style="@style/txtPilatExtendedheavy12" tools:text="@string/lbl_barcelona"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtZero"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_105pxh"
                            android:layout_weight="0.14"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{futbolRowModel.txtZero}"
                            android:textAllCaps="true"
                            style="@style/txtPilatExtendedheavy16" tools:text="@string/lbl_0"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowone"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowmadird"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.92"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageMadird"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_width="@dimen/_15pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_madird"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_madird"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtRealMadrid"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_weight="0.87"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{futbolRowModel.txtRealMadrid}"
                                style="@style/txtPilatExtendedheavy12" tools:text="@string/lbl_real_madrid"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_91pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.08"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{futbolRowModel.txtOne}"
                            android:textAllCaps="true"
                            style="@style/txtPilatExtendedheavy16" tools:text="@string/lbl_1"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowsave"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageSave"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_13pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_save"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_save"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt4930"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{futbolRowModel.txt4930}"
                            style="@style/txtSFUIDisplayregular10" tools:text="@string/lbl_49_30"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnsettings2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_weight="0.13"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSevenHundredNinety"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{futbolRowModel.txtSevenHundredNinety}"
                        style="@style/txtLatobold10" tools:text="@string/lbl_790"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowframe245"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame245"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{futbolRowModel.txtFrame245}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_1_8"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame246"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{futbolRowModel.txtFrame246}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_2_1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame247"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{futbolRowModel.txtFrame247}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_1_3"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
