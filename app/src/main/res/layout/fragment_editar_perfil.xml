<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearMisdatosThree"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="@color/">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearMisdatos"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical">
            <ImageView android:id="@+id/imageLock"
                android:layout_gravity="center_horizontal"
                android:layout_height="25dp"
                android:layout_marginEnd="41dp"
                android:layout_marginStart="41dp"
                android:layout_marginTop="31dp"
                android:layout_width="25dp"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"
                android:src="@drawable/img_lock"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMisdatos"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="41dp"
                android:layout_marginStart="41dp"
                android:layout_marginTop="1dp"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_mis_datos"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearFOTO"
                android:layout_height="wrap_content"
                android:layout_marginEnd="41dp"
                android:layout_marginStart="41dp"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <FrameLayout android:id="@+id/frameFOTO"
                    android:layout_gravity="start"
                    android:layout_height="91dp"
                    android:layout_width="91dp">
                    <ImageView android:id="@+id/imageFOTO"
                        android:layout_gravity="center"
                        android:layout_height="91dp"
                        android:layout_marginEnd="1dp"
                        android:layout_width="91dp"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_foto}"
                        tools:ignore="ContentDescription"
                        android:src="@drawable/img_foto"/>
                    <ImageButton
                        android:background="@drawable/layer_list_bg_gray_500_radius_13"
                        android:id="@+id/btnThumbsup"
                        android:layout_gravity="bottom|end"
                        android:layout_height="27dp"
                        android:layout_marginBottom="4dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="26dp"
                        android:paddingHorizontal="9dp"
                        android:paddingVertical="9dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_thumbsup"
                        tools:ignore="ContentDescription"/>
                </FrameLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnEditarDatos"
                android:layout_height="wrap_content"
                android:layout_marginEnd="41dp"
                android:layout_marginStart="41dp"
                android:layout_marginTop="12dp"
                android:layout_width="match_parent"
                android:paddingBottom="12dp"
                android:paddingEnd="30dp"
                android:paddingStart="30dp"
                android:paddingTop="12dp"
                android:singleLine="true"
                android:textAllCaps="false"
                android:text="@string/lbl_editar_datos"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearNOME"
                android:layout_height="wrap_content"
                android:layout_marginEnd="41dp"
                android:layout_marginStart="41dp"
                android:layout_marginTop="12dp"
                android:layout_width="match_parent"
                android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_red_901_radius_6_14"
                    android:id="@+id/btnLockOne"
                    android:layout_height="28dp"
                    android:layout_width="28dp"
                    android:paddingHorizontal="8dp"
                    android:paddingVertical="8dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_lock_white_a700"
                    tools:ignore="ContentDescription"/>
                <FrameLayout android:id="@+id/frameStackpeterparker"
                    android:layout_height="21dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="3dp" android:layout_width="199dp">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPeterParker"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="1dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="2dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_peter_parker"/>
                    <EditText android:ellipsize="end"
                        android:hint="@string/lbl_your_name"
                        android:id="@+id/etGroupTwo"
                        android:inputType="text"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="13dp"
                        android:paddingEnd="30dp"
                        android:paddingStart="2dp"
                        android:singleLine="true"
                        android:text="@={misDatosThreeVM.misDatosThreeModel.etGroupTwoValue}"
                        android:textColorHint="@color/gray"
                        style="@style/etSoraregular6" tools:ignore="Autofill"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearEMAIL"
                android:layout_height="wrap_content"
                android:layout_marginEnd="41dp"
                android:layout_marginStart="41dp"
                android:layout_marginTop="9dp"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_red_901_radius_6_14"
                    android:id="@+id/btnLockTwo"
                    android:layout_height="28dp"
                    android:layout_width="28dp"
                    android:paddingHorizontal="8dp"
                    android:paddingVertical="8dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_lock_white_a700_28x28" tools:ignore="ContentDescription"/>
                <FrameLayout android:id="@+id/frameStackemail"
                    android:layout_height="21dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginStart="11dp"
                    android:layout_marginTop="3dp" android:layout_width="196dp">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmail"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="1dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{misDatosThreeVM.misDatosThreeModel.txtEmail}"
                        style="@style/txtSoraregular8" tools:text="@string/msg_spiderman_gmail"/>
                    <EditText android:ellipsize="end"
                        android:hint="@string/lbl_your_email"
                        android:id="@+id/etGroupFour"
                        android:inputType="textEmailAddress"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="13dp"
                        android:paddingEnd="30dp"
                        android:paddingStart="3dp"
                        android:singleLine="true"
                        android:text="@={misDatosThreeVM.misDatosThreeModel.etGroupFourValue}"
                        android:textColorHint="@color/gray_700"
                        style="@style/etSoraregular6" tools:ignore="Autofill"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearSEENHA"
                android:layout_height="wrap_content"
                android:layout_marginBottom="26dp"
                android:layout_marginEnd="41dp"
                android:layout_marginStart="41dp"
                android:layout_marginTop="9dp"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_red_901_radius_6_14"
                    android:id="@+id/btnTrophy"
                    android:layout_height="28dp"
                    android:layout_width="28dp"
                    android:paddingHorizontal="8dp"
                    android:paddingVertical="8dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_trophy" tools:ignore="ContentDescription"/>
                <FrameLayout android:id="@+id/frameStackpassword"
                    android:layout_height="20dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginStart="11dp"
                    android:layout_marginTop="3dp" android:layout_width="196dp">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPassword"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="3dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{misDatosThreeVM.misDatosThreeModel.txtPassword}"
                        style="@style/txtSoraregular8" tools:text="@string/lbl"/>
                    <LinearLayout
                        android:id="@+id/linearColumnyourpassword"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowyourpassword"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="10dp"
                            android:layout_marginStart="3dp"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtYourpassword"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="7dp"
                                android:layout_weight="0.81"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{misDatosThreeVM.misDatosThreeModel.txtYourpassword}"
                                style="@style/txtSoraregular6_1" tools:text="@string/lbl_your_password"/>
                            <ImageView android:id="@+id/imageEye"
                                android:layout_height="8dp"
                                android:layout_marginStart="116dp"
                                android:layout_marginTop="6dp"
                                android:layout_width="10dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_eye"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                        </LinearLayout>
                        <View android:background="@color/red_901"
                            android:id="@+id/lineRectangle4270"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp" android:layout_width="196dp"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
